@{
    Layout = null;
    DIMS.Models.UserProperties UP = (DIMS.Models.UserProperties)Session["UP"];

    string SessionUserCode_ICC = UP.UserCode;
    string UserType_ICC = UP.UserTypeCode;
}

<style>
    table tr {
        cursor: pointer;
    }
</style>
<section class="content-header" style="padding: 1px 15px 0;" id="CMS_ListMenu_ICC">
    <h2>Complaint Registration ICC</h2>
    <ol class="breadcrumb">
        <li><a href="#" ng-click="go('home')"><i class="fa fa-dashboard"></i>Home</a></li>
        <li>Complaint Management</li>
        <li class="active">Complaint Registration ICC</li>
    </ol>
    <nav style="padding: 1px 10px 1px 10px;">
        <div class="row">
            <div class="col-sm-11" id="IconsNavBar">
                <a id="CreateNewComplaint_ICC" href ng-click="go('Registration_ICC')" style="float:left;"> <i class="fa fa-file-o fa-4" title="Create New Complaint"></i> </a>
                <select id="StateFilter_ICC" ng-model="StateFilter_ICC" ng-change="StateChange_ICC()" style="float:left;"></select>
            </div>
        </div>
    </nav>
</section>
<div class="CustomHeight_Section" style="overflow: auto;">
    <section style="padding:0 15px 90px;" id="CMS_List">
        <div class="row">
            <div class="col-md-12">
                <div class="box box-primary" style="background: white none repeat scroll 0 0; padding: 4px;">
                    <div class="box-header" style="overflow-x: scroll; width: 98%;" id="RegistrationListDiv_ICC">
                        <input type="text" id="UserCode_ICC" value="@SessionUserCode_ICC" style="display: none;" />
                        <input type="text" id="UserType_ICC" value="@UserType_ICC" style="display: none;" />
                        <table id="RegistrationList_ICC" class="table ListTable" width="100%" cellspacing="0">
                            <thead>
                                <tr>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                        <input type="button" value="Advanced" id="AdvancedColumnEditing_ICC" style="background-color: #d2401a; display: none;" class="smallbutton" data-toggle="modal" data-target="#" ng-click="OpenColumnEditing_ICC()" />
                    </div>
                </div>
            </div>
        </div>
    </section>
</div>

<script>
    $(window).resize(function () {
        $('.CustomHeight_Section').height($(window).height() - 170);
    });
    $('.CustomHeight_Section').height($(window).trigger('resize'));
</script>