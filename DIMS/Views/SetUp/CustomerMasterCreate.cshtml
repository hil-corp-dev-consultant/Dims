@using LabelName = DIMS.Content.Resource_Files.CustomerMaster;
@{
    Layout = null;
}
<section class="content-header" style="padding: 1px 15px 0;">
    <h2>Customer Master</h2>
    <ol class="breadcrumb">
        <li><a href ng-click="go('home')"><i class="fa fa-dashboard"></i>Home</a></li>
        <li>Administration</li>
        <li>Setup</li>
        <li>Customers</li>
        <li class="active">Customer Master</li>
    </ol>
    <nav style="padding: 1px 10px 1px 10px;">
        <div class="row">
            <div class="col-sm-11" id="IconsNavBar">

                <a href ng-click="SaveCUSTOMERFunction()" id="CompRegSave">
                    <i class="fa fa-floppy-o fa-4" title="Save"></i>
                </a>
                <a href ng-click="listcustomerFunction()">
                    <i class="fa fa-list fa-4" title="List"></i>
                </a>
                <a href ng-click="NewcustomerFunction()">
                    <i class="fa fa-file-o fa-4" title="Add New"></i>
                </a>

            </div>
        </div>
    </nav>
</section>
<section class="content" style="max-height: 478px; overflow-y: scroll;">
    <div class="box-header">
    </div>
    @*<input type="button" id="save" value="Add" ng-click="SaveCUSTOMERFunction()" style="background-color: #3295D6" />*@
    @*<button type="button" id="save" class="btn btn-primary" ng-click="SaveCUSTOMERFunction()" style="background-color: #3295D6">Save</button>
    <button type="button" id="List" class="btn btn-primary" ng-click="listcustomerFunction()" style="background-color: #3295D6">List</button>

    <button type="button" id="New" class="btn btn-primary" ng-click="NewcustomerFunction()" style="background-color: #3295D6">New</button>*@


    @*    <input type="button" value="List" ng-click="listcustomerFunction()" style="background-color: #3295D6" />
    <input type="button" value="New" ng-click="NewcustomerFunction()" style="background-color: #3295D6" />*@

    <div class="row" id="addcustomerview">
        <div class="col-md-12">
            <div class="box box-primary" style="background: white none repeat scroll 0 0; padding: 4px;">
                <div class="box-header">
                    @*<h3 class="box-title">Complaint Registration</h3>*@
                </div>
                <div class="box-body pad">
                    <form class="form-horizontal" role="form" style="font-size: 12px; font-weight: normal" action="">
                        <input type="hidden" id="customermaster_id" data-ng-model="customermaster_id_Model" />

                        <div class="col-sm-12">
                            <div class="col-sm-6">
                                <div class="row">
                                    <div class="form-group">
                                        <label class="col-sm-5 control-label" for="txtCustomerCode">
