    <div class="row" id="listcustomer">
        <div class="col-md-12">
            <div class="box box-primary" style="background: white none repeat scroll 0 0; padding: 4px;">
                <div class="table-responsive">
                    <table id="listcustomerview" class="table" width="100%" cellspacing="0">
                        <thead>
                            <tr>
                                <td></td>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</section>
