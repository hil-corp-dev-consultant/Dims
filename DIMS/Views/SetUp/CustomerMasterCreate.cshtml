                                        <div class="col-sm-6">
                                            <input type="text" class="form-control" id="txtCustomerOldCode" ng-model="txtCustomerOldCode_Model" />
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="form-group">
                                        <label class="col-sm-5 control-label" for="txtContactName1">
                                            @LabelName.ContactPerson1Name
                                        </label>
                                        <div class="col-sm-6">
                                            <input type="text" class="form-control" id="txtContactName1" ng-model="txtContactName1_Model" />
