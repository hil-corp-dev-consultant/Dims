<script type="text/javascript">


    $(function ()
    {
        $("#complaintReceivedDate_CRT").datepicker({
            format: 'dd/mm/yyyy',
            autoclose: true
        });
        $("#ComplaintRegistrationDate_CRT").datepicker({
            format: 'dd/mm/yyyy',
            autoclose: true
        });
        $("#Approved_Date").datepicker({
            format: 'dd/mm/yyyy',
            autoclose: true
        });

    });


    function FormToggle(DivName)
    {
        try
        {
            if ($("#" + DivName + "_Form").css("display") == "none")
            {
                $("#" + DivName + "_Form").css("display", "block");
                $("#" + DivName + "_Img").attr("src", "../../Images/en_expend.png");
            }
            else if ($("#" + DivName + "_Form").css("display") == "block")
            {
                $("#" + DivName + "_Form").css("display", "none");
                $("#" + DivName + "_Img").attr("src", "../../Images/en_next.png");
            }
        }
        catch (e)
        {
            alert("Error : " + e);
        }
    }


    function SaveFunction()
    {
        console.clear();
        try
        {

            var complaintReceivedDate_CRT = $("#complaintReceivedDate_CRT").val();
            var ComplaintRegistrationDate_CRT = $("#ComplaintRegistrationDate_CRT").val();
            var Registrant_Type = $("#Registrant_Type").val();
