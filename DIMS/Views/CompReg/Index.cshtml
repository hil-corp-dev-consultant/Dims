            var Customer_Name = $("#Customer_Name").val();
            var Customer_Type = $("#Customer_Type").val();
            var Customer_Location = $("#Customer_Location").val();
            var Contact_City = $("#Contact_City").val();
            var Contact_State = $("#Contact_State").val();
            var Contact_Area = $("#Contact_Area").val();

            var Contact_Person = $("#Contact_Person").val();
            var Contact_Number = $("#Contact_Number").val();
            var Customer_Fax = $("#Customer_Fax").val();
            var Customer_Email = $("#Customer_Email").val();
            var Complaint_Mode = $("#Complaint_Mode").val();
            var Product_Type = $("#Product_Type").val();
            var Product_Category = $("#Product_Category").val();

            var SubStockiest_Direct_Customer = $("#SubStockiest_Direct_Customer").val();
            var Is_Project_Party = $("#Is_Project_Party").val();
            var Site_Address = $("#Site_Address").val();




            var Complaint_Type = $("#Complaint_Type").val();
            var Complaint_Category = $("#Complaint_Category").val();
            var Complaint_Priority = $("#Complaint_Priority").val();
            var Complaint_Severity = $("#Complaint_Severity").val();


            //Material_Supply_Detail_Table                //Table
            //Receipt_Details_Sheeting_Table              //Table




            var Flag = 0;

            if (complaintReceivedDate_CRT == "")
            {
                Flag = Flag + 1;
                $("#complaintReceivedDate_CRT").css("border-color", "red");
            }
            else
            {
                $("#complaintReceivedDate_CRT").css("border-color", "#d2d6de");
            }

            if (Registrant_Type == "")
            {
                Flag = Flag + 1;
                $("#Registrant_Type").css("border-color", "red");
            }
